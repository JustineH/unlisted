                <span class="bathroom">2 Bathrooms</span>
                <span class="size">2,214 sq ft</span>
              </p>
            </div>
          </div>
        </div>
      </div>
              </div>
              
              <div id="profile-listing-btn-wrap">
                <% current_user.listings.each do |listing| %>
                 <%= link_to "Edit", edit_listing_path(listing), id: "profile-edit-btn" %>
                 <%= link_to "Delete", listing_path(listing), id: "profile-delete-btn", method: :delete %>
                 <% end %>
              </div> <!-- end profile-listing-btn-wrap -->
            </div>
            <!-- end profile user listing div -->
          </div>
          <!-- end profile-listing-wrap -->
        </div>
        <!-- end container (for listings) -->
