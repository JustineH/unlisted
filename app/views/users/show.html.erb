    <div class="col-md-9 col-sm-9 col-xs-12 ">
      <!-- main profile content -->


      <div id="main-content-wrap">
        <div class="container">
          <!-- container (for listings) -->


          <div class="profile-listing-wrap">
            <div id="listing-head-div">
              <h3>Your Listings</h3>
            </div>
            <!-- end listing-head-div -->


            <div id="profile-user-listing-div">
              
              <div id="profile-user-listing-img-wrap">
                 <div class="featureList text-center">
        <div id="carousel-listing-2" class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
            <div class="item active">
              <%= image_tag "listing_images/1.jpg", class: 'img-responsive' %>
            </div>
            <div class="item">
              <%= image_tag "listing_images/17.jpg", class: 'img-responsive' %>
            </div>
            <div class="item">
              <%= image_tag "listing_images/3.jpg", class: 'img-responsive' %>
            </div>
          </div>
          <a class="left carousel-control c-control" href="#carousel-listing-2" role="button" data-slide="prev">
            <span class="icon-prev" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="right carousel-control c-control" href="#carousel-listing-2" role="button" data-slide="next">
            <span class="icon-next" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
