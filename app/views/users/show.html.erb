      </div> -->
    </div>
    <!-- profile jumbo end -->


    <div class="col-md-3 col-sm-3 col-xs-12 no-padding-profile">
      <!-- profile-side-nav -->


      <div id="profile-side-nav-wrap">
        <div id="user-profile-name">
          <h2><%=User.find(current_user.id).firstname %></h2></div><!-- end user profile name -->

        <div id="profile-nav-home">
          <a href="/"><i aria-hidden="true" class="fa fa-home fa-2x"></i><br>
          Home</a>
