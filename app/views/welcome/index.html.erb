<%= render 'layouts/header' %>



<!-- Carousel Begins-->

<section class="carousel slide carousel-fade anchor" data-ride="carousel">
  <div id="welcome-carousel">
    <div class="shadow-layer"></div>
    <div class="carousel-inner">
      <div class="item active">
        <%= image_tag "welcome_bg1_1900_1200.png" %>
      </div>
      <div class="item">
        <%= image_tag "welcome_bg2_1900_1200.png" %>
      </div>
      <div class="item">
        <%= image_tag "welcome_bg3_1900_1200.png" %>
      </div>
    </div>
  </div>
</section>

<!-- Carousel Ends-->

<!-- Search Box Cluster Begins-->
<h1 id="tagline">The easiest way to list and buy a home.</h1>
<%= render 'layouts/search' %>

<!-- Search Box Cluster Ends -->



<!-- Choose Us Begins -->

<section id="choose-us" class="anchor">

  <div class="row">
    <h3 class="heading">Why Unlisted?</h3>
    <h5 class="sub-heading"></h5>
    <div class="col-md-4 col-sm-6 cols-xs-12">
     <i class="fa fa-usd fa-5x" aria-hidden="true"></i>
     <h4 class="heading">Make Money</h4>
     <p>Putting realtor fees back in your pocket</p>
   </div>
   <div class="col-md-4 col-sm-6 cols-xs-12">
    <i class="fa fa-clock-o fa-5x" aria-hidden="true"></i>
    <h4 class="heading">Save Time</h4>
    <p>Sell your home on your own terms and at your convenience</p>
  </div>

  <div class="col-md-4 col-sm-6 cols-xs-12">
    <i class="fa fa-paper-plane fa-5x" aria-hidden="true"></i>
    <h4 class="heading">Easy to List</h4>
    <p>List your home in under 5 minutes</p>
  </div>
</div>
</section>

<!-- Choose Us Ends -->




