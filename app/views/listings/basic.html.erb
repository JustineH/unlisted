<%= form_for @listing do |f| %>
    <div class="form-row">
        <%= f.label :unit_number, "Unit number" %>
        <%= f.text_field :unit_number %>
    </div>
     <div class="form-row">
        <%= f.label :street_name, "Street name" %>
        <%= f.text_field :street_name %>
    </div>
    <div class="form-row">
        <%= f.label :postal_code, "Postal Code" %>
        <%= f.text_field :postal_code %>
    </div>
    <div class="form-row">
        <%= f.label :city %>
        <%= f.text_field :city %>
    </div>
    <div class"form-row">
        <%= f.label :neighbourhood_id, "Neighbourhood" %>
        <%= f.collection_select :neighbourhood_id, Neighbourhood.all, :id, :name%>
    </div>
    <div class="form-row">
          <button type="submit">Submit Form</button>

<%end%>

  <div id="map"></div>
  <!-- </div> -->



<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDsxtuexS3LgdiMGA4Z5Syb2ZHGy9_1b3s&callback=initMap"></script>

